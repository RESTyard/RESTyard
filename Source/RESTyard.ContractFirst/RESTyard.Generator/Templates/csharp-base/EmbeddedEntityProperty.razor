@inherits RazorTemplateBase

@{
    var documentType = $"<{EntityType.document}Hto>";
    var entityType = $"List<IEmbeddedEntity{documentType}>";
}

<Attribute>Relations(["@EntityType.collectionName"])</Attribute>
<PropertyStatement TypeName="@entityType" PropertyName="@Capitalize(EntityType.collectionName)" />

@code {
    [Parameter] public EntityType EntityType { get; set; } = null!;
}